{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "publicPath": "/wordpress/site-psm-back/wp-content/themes/sage-theme-psm",
  "devUrl": "http://localhost/wordpress/site-psm-back",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "__comment" : "For production, remove _[hash:8] of cacheBusting.",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
